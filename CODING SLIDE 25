public class Slide25 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age = 30;
		if  ((age >= 25) && (age <= 50))
		{
			System.out.println("You are qualified to apply");
		}
		else 
		{
		     System.out.println("You are NOT qualified to apply"); 
		}


	}

}
